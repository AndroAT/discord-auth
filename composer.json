    "autoload": {
        "psr-4": {
            "Azuriom\\Plugin\\DiscordAuth\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/socialite": "^5.0",
        "martinbean/socialite-discord-provider": "^1.2"
